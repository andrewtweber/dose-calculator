{
    "name": "andrewtweber/dose-calculator",
    "type": "library",
    "description": "Dose calculator",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Weber",
            "homepage": "https://andrew.cool"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-bcmath": "*",
        "illuminate/database": "^8.12|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "larastan/larastan": "^1.0",
        "orchestra/testbench": "^6.17",
        "phpunit/phpunit": "^9"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "DoseCalculator\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DoseCalculator\\Tests\\": "tests/"
        }
    }
}
